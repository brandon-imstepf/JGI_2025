/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/gmhmmp2 -M /clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_0.lst 
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_0.lst -m itr_1.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 0 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --cpus 10 --run-motif-search false --genome-group D
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/gmhmmp2 -m itr_1.mod -M /clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_1.lst --format train
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/compp -n -a itr_0.lst -b itr_1.lst -G
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --cpus 10 --genome-group C --gc-upstr-len-rbs 20 --align right --gc-width-rbs 6 --gc-upstr-len-prom 20 --gc-width-prom 6 --gc-extended-sd TAAGGAGGTGA
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/gmhmmp2 -m itr_2.mod -M /clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/compp -n -a itr_1.lst -b itr_2.lst -G
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/biogem experiment promoter-is-valid-for-bacteria --fnmod itr_2.mod --dist-thresh 14 --score-thresh 0.25 --window-size 1 --min-leaderless-percent 11 --min-leaderless-count 100 --fnlabels itr_2.lst --fnseq tmpseq.fna
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/biogem experiment promoter-and-rbs-match --fnmod itr_2.mod --match-thresh 3
