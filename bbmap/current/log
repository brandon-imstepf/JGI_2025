/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/gmhmmp2 -M /clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_0.lst 
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/biogem gms2-training -s tmpseq.fna -l itr_0.lst -m itr_1.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 0 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --cpus 10 --run-motif-search false --genome-group D
/clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/gmhmmp2 -m itr_1.mod -M /clusterfs/jgi/scratch/gentech/genome_analysis/brandonimstepf/comparison_models/genemark/gms2_linux_64/mgm_11.mod -s tmpseq.fna -o itr_1.lst --format train
